{
  "name": "spaceonfire/data-source",
  "type": "library",
  "description": "",
  "keywords": [
    "spaceonfire",
    "DataSource",
    "DDD"
  ],
  "homepage": "https://github.com/spaceonfire/data-source",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://www.onfire.space",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/spaceonfire/criteria.git"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "guzzlehttp/psr7": "^1.6.1",
    "jawira/case-converter": "^3.4.1",
    "spaceonfire/collection": "^1.2.1",
    "spaceonfire/criteria": "dev-master",
    "webmozart/assert": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-webmozart-assert": "^0.12",
    "roave/security-advisories": "dev-master",
    "symplify/easy-coding-standard-prefixed": "^7.2",
    "spiral/pagination": "^2.2.2"
  },
  "suggest": {
    "spiral/pagination": "Needed for QueryInterface"
  },
  "autoload": {
    "psr-4": {
      "spaceonfire\\DataSource\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spaceonfire\\DataSource\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "codestyle": "ecs check --ansi",
    "lint": "phpstan analyze --memory-limit=512M --ansi"
  },
  "config": {
    "sort-packages": true
  }
}
