{
  "name": "spaceonfire/data-source",
  "type": "library",
  "description": "",
  "keywords": [
    "spaceonfire",
    "DataSource",
    "DDD"
  ],
  "homepage": "https://github.com/spaceonfire/data-source",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://www.onfire.space",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/psr7": "^1.6.1",
    "jawira/case-converter": "^3.4.1",
    "spaceonfire/collection": "^1.2.1",
    "webmozart/assert": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "squizlabs/php_codesniffer": "^3.0",
    "doctrine/collections": "^1.6.4",
    "spiral/pagination": "^2.2.2"
  },
  "suggest": {
    "doctrine/collections": "Needed for Criteria",
    "spiral/pagination": "Needed for Criteria and QueryInterface"
  },
  "autoload": {
    "psr-4": {
      "spaceonfire\\DataSource\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spaceonfire\\DataSource\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
